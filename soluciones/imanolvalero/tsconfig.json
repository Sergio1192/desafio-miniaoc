{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "sourceMap": false, 
        "jsx": "react",
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": ["**/*"],
    "exclude": ["node_modules"]
}